{
  "description": "Finds the Answers for you! ;)",

  "manifest_version": 2,
  "name": "AnsFinder",
  "version": "3.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon.png"
  },
  "permissions": ["activeTab"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["AnsFinder.js"]
    }
  ]
}
